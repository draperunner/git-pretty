  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^5.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
